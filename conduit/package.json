{
  "name": "moleculer-examples-conduit",
  "version": "2.0.0",
  "description": "RealWorld example app with Moleculer microservices framework",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "start": "moleculer-runner",
    "deps": "npm-check -u",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "docker:build": "docker build -t conduit ."
  },
  "keywords": [
    "microservices",
    "moleculer",
    "realworld"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "5.0.1",
    "jest": "23.3.0",
    "jest-cli": "23.3.0",
    "moleculer-repl": "0.5.0",
    "npm-check": "5.7.1"
  },
  "dependencies": {
    "bcrypt": "2.0.1",
    "ioredis": "3.2.2",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.10",
    "moleculer": "0.13.0",
    "moleculer-console-tracer": "0.1.1",
    "moleculer-db": "0.7.3",
    "moleculer-db-adapter-mongo": "0.4.0",
    "moleculer-web": "0.8.0",
    "slug": "0.9.1",
    "startbootstrap-agency": "^5.0.2"
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
